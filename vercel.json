{
    "version": 2,
    "builds": [
        {
            "src": "api/index.ts",
            "use": "@vercel/node"
        },
        {
            "src": "frontend/package.json",
            "use": "@vercel/next"
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "api/index.ts"
        },
        {
            "source": "/(.*)",
            "destination": "frontend/$1"
        }
    ]
}